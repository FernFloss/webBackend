# Database Configuration
# PostgreSQL is running in Docker container with port 5432 published to host
# From host machine, use localhost (not 'postgres' container name)
DB_HOST=postgres
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=auditorium_db

# RabbitMQ Configuration
# RabbitMQ is running in Docker container with port 5672 published to host
# From host machine, use localhost (not 'rabbitmq' container name)
# Format: amqp://username:password@host:port/vhost
RABBITMQ_URL=amqp://rabbitmq_user:rabbitmq_pass@rabbitmq:5672/
QUEUE_NAME=camera_events

# Server Configuration
GIN_MODE=release
SERVER_PORT=8080
